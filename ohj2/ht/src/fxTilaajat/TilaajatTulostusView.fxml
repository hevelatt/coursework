<?xml version="1.0" encoding="UTF-8"?>

<?import fi.jyu.mit.fxgui.CheckBoxChooser?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxTilaajat.TilaajatTulostusController">
   <center>
      <ScrollPane>
         <content>
            <CheckBoxChooser fx:id="cbKentat" />
         </content>
         <BorderPane.margin>
            <Insets bottom="10.0" top="10.0" />
         </BorderPane.margin>
      </ScrollPane>
   </center>
   <top>
      <Label text="Valitse tulostettavat:" />
   </top>
   <bottom>
      <ButtonBar>
        <buttons>
            <Button defaultButton="true" mnemonicParsing="false" onAction="#handleOK" text="OK" />
            <Button cancelButton="true" mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
